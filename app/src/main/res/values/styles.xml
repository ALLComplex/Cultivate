<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CultivateTheme" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/that_green</item>
        <item name="colorPrimaryDark">@color/that_dark_green</item>
        <item name="colorAccent">@color/that_logo_brown</item>
        <item name="android:colorBackground">@color/that_light_goldenyellow</item>
        <item name="android:colorForeground">@color/that_logo_goldenyellow</item>
        <item name="android:imageButtonStyle">@drawable/ic_action_name</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondaryInverse">@color/that_logo_goldenyellow</item>
        <item name="android:windowBackground">?android:attr/textColorSecondaryInverse</item>
        <item name="android:textColorPrimaryInverse">@color/that_logo_goldenyellow</item>
    </style>


</resources>
